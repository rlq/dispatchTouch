<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >



    <com.lq.ren.dispatch.IndicatorView
        android:id="@+id/dispatch_indicatorView"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        />

    <android.support.v4.view.ViewPager
        android:id="@+id/dispatch_viewpager"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/dispatch_indicatorView" />

    <com.lq.ren.dispatch.UpView
        android:id="@+id/dispatch_upview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="invisible"/>

    <com.lq.ren.dispatch.DownView
        android:id="@+id/dispatch_downview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="invisible"/>

    <include layout="@layout/custom_dispatch_pause"/>

</RelativeLayout>
